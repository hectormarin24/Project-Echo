                <Text fill="#ad6449" layoutX="147.0" layoutY="372.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Binyi Chen" />
                <Text fill="#ad6449" layoutX="276.0" layoutY="372.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Kaylina Savatdy" />
                <Text fill="#ad6449" layoutX="433.0" layoutY="372.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ethan Richard" />
                <Text fill="#ad6449" layoutX="571.0" layoutY="372.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nicholas Safran" />

